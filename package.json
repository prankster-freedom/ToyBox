{
    "name": "toybox",
    "version": "1.1.0",
    "description": "A simple API server with AI agent.",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "start": "cross-env NODE_ENV=development node index.js",
        "dev": "npm run start:emulator",
        "start:prod": "cross-env NODE_ENV=production node index.js",
        "start:emulator": "cross-env NODE_ENV=development IS_LOCAL=true DATASTORE_EMULATOR_HOST=localhost:8081 node index.js",
        "job:dream": "node jobs/dream.js"
    },
    "dependencies": {
        "@google-cloud/datastore": "^8.0.0",
        "@google-cloud/tasks": "^5.0.0",
        "@google/genai": "^1.29.1",
        "cross-env": "^10.1.0",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "passport": "^0.6.0",
        "passport-google-oauth20": "^2.0.0",
        "uuid": "^13.0.0"
    },
